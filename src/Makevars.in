RMonetDB_CPPFLAGS=@RMonetDB_CPPFLAGS@
PKG_CPPFLAGS=@PKG_CPPFLAGS@
PKG_LIBS=@LIBS@

all: $(SHLIB)

$(SHLIB): RMonetDB.o

RMonetDB.o: RMonetDB.c
	$(CC) -c  $(PKG_CPPFLAGS) $(CPPFLAGS) $(CFLAGS)  $(RMonetDB_CPPFLAGS) $(OPTIMIZE) $(MPOLLUTE) $(CCCDLFLAGS) -D_REENTRANT -D_GNU_SOURCE -DDEBIAN -fno-strict-aliasing -pipe -fstack-protector -I/usr/local/include -D_LARGEFILE_SOURCE -D_FILE_OFFSET_BITS=64 -O2 -g  -fPIC -D__NO_MATH_INLINES -fPIC  RMonetDB.c $(PKG_LIBS)



